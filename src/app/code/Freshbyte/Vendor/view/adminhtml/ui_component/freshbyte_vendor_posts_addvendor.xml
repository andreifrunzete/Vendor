<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd" extends="pagebuilder_base_form_with_background_attributes">

    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">freshbyte_vendor_posts_addvendor.freshbyte_vendor_data_source</item>
        </item>
        <item name="label" xsi:type="string" translate="true">New Vendor</item>
    </argument>

    <settings>
        <buttons>
            <button name="save_and_continue" class="Magento\CatalogRule\Block\Adminhtml\Edit\SaveAndContinueButton"/>
            <button name="reset" class="Magento\CatalogRule\Block\Adminhtml\Edit\ResetButton"/>
            <button name="back">
                <url path="*/*/"/>
                <class>back</class>
                <label translate="true">Back</label>
            </button>
        </buttons>

<!--        <namespace>freshbyte_vendor_posts_addvendor</namespace>-->
        <deps>
            <dep>freshbyte_vendor_posts_addvendor.freshbyte_vendor_data_source</dep>
        </deps>
<!--        <argument name="data" xsi:type="array">-->
<!--            <item name="label" xsi:type="string" translate="true">Vendors</item>-->
<!--        </argument>-->
        <layout>

            <type>tabs</type>
        </layout>
    </settings>

    <dataSource name="freshbyte_vendor_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Freshbyte\Vendor\Model\DataProvider</argument>
            <argument name="name" xsi:type="string">freshbyte_vendor_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">id</item>
                    </item>
                </item>
            </argument>
        </argument>
<!--        <argument name="data" xsi:type="array">-->
<!--            <item name="js_config" xsi:type="array">-->
<!--                <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>-->
<!--            </item>-->
<!--        </argument>-->
<!--        <settings>-->
<!--            <submitUrl path="adminhtml/posts/save"/>-->
<!--        </settings>-->
<!--        <dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="freshbyte_vendor_vendor_listing_data_source">-->
<!--            <settings>-->
<!--                <requestFieldName>id</requestFieldName>-->
<!--                <primaryFieldName>id</primaryFieldName>-->
<!--            </settings>-->
<!--        </dataProvider>-->


    </dataSource>




<!--    <settings>-->
<!--        <tab id="A_UNIQUE_ID" translate="label" class="a-custom-css-class-to-style-this-tab" sortOrder="10">-->
<!--            <label>Vendor</label>-->
<!--        </tab>-->
<!--    </settings>-->

    <fieldset name="tab1" sortOrder="10" component="Magento_PageBuilder/js/form/element/dependent-fieldset">
        <settings>
            <label translate="true">Vendor</label>
        </settings>



                <field name="is_enable" component="Magento_Catalog/js/components/visible-on-option/yesno" formElement="checkbox" sortOrder="10">
                    <argument name="data" xsi:type="array">
                        <item name="config" xsi:type="array">
                            <item name="valuesForOptions" xsi:type="array">
                                <item name="boolean" xsi:type="string">boolean</item>
                            </item>
                            <item name="default" xsi:type="number">1</item>
                        </item>
                    </argument>
                    <settings>
                        <dataType>boolean</dataType>
                        <label translate="true">Enable</label>
                        <dataScope>is_enable</dataScope>
                    </settings>
                    <formElements>
                        <checkbox>
                            <settings>
                                <valueMap>
                                    <map name="false" xsi:type="number">0</map>
                                    <map name="true" xsi:type="number">1</map>
                                </valueMap>
                                <prefer>toggle</prefer>
                            </settings>
                        </checkbox>
                    </formElements>
                </field>

                <field name="name" formElement="input" sortOrder="30">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Name</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>

                <field name="email" formElement="input" sortOrder="40">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Email</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>

                <field name="telephone" formElement="input" sortOrder="50">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Telephone</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>




    </fieldset>


</form>
