    <fieldset name="tab1">
        <sections id="A_UNIQUE_SECTION_ID" showInDefault="1" showInWebsite="0" showInStore="1" sortOrder="10" translate="label">
          <section>
            <label>Vendor Details</label>
            <tab>A_UNIQUE_ID</tab>
            <resource>VENDOR_MODULE::path_to_the_acl_resource</resource>

            <group id="A_UNIQUE_GROUP_ID" translate="label" sortOrder="20" showInDefault="1" showInWebsite="0" showInStore="1">
                <label>Vendor Details</label>
                <comment>An additional comment helping users to understand the effect when configuring the fields defined in this group.</comment>
                <settings>
                    <label>Vendor Details</label>
                </settings>
                <field name="is_enable" component="Magento_Catalog/js/components/visible-on-option/yesno" formElement="checkbox" sortOrder="10">
                    <argument name="data" xsi:type="array">
                        <item name="config" xsi:type="array">
                            <item name="valuesForOptions" xsi:type="array">
                                <item name="boolean" xsi:type="string">boolean</item>
                            </item>
                            <item name="default" xsi:type="number">1</item>
                        </item>
                    </argument>
                    <settings>
                        <dataType>boolean</dataType>
                        <label translate="true">Enable</label>
                        <dataScope>is_enable</dataScope>
                    </settings>
                    <formElements>
                        <checkbox>
                            <settings>
                                <valueMap>
                                    <map name="false" xsi:type="number">0</map>
                                    <map name="true" xsi:type="number">1</map>
                                </valueMap>
                                <prefer>toggle</prefer>
                            </settings>
                        </checkbox>
                    </formElements>
                </field>

                <field name="input_name" formElement="input" sortOrder="30">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Name</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>

                <field name="input_email" formElement="input" sortOrder="40">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Email</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>

                <field name="input_telephone" formElement="input" sortOrder="50">
                    <settings>
<!--                        <visible>true</visible>-->
<!--                        <elementTmpl>ui/form/element/input</elementTmpl>-->
                        <label translate="true">Telephone</label>
<!--                        <item name="validate-number" xsi:type="boolean">true</item>-->
                    </settings>
                </field>


            </group>
          </section>

